<h1>Add Players</h1>

<%= form_for @player do |p| %>

<p>
		<%= p.label :name%>
		<%= p.text_field :name %>
</p>

<p>
		<%= p.label :points, "Points"%>
		<%= p.text_field :points %>
</p>

<p>
		<%= p.label :tournament, "Events"%>
		<%= p.text_field :tournament %>
</p>



<p>
<%= p.submit%>
</p>

<%end%>